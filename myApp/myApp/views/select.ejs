<h1>Books Data</h1>

<a href="/form">Form Insert</a>

<table>
  <tr>
     <td>id</td>
     <td>Name</td>
     <td>Price</td>
  </tr>
   <% books.forEach(function(item) { %>
  <tr>
     <td><%= item.id %></td>
     <td><%= item.name %></td>
     <td><%= item.price %></td>
     <td>
       <a href="/delete?id=<%= item.id %>" onClick="return confirm('Are you sure')">Delete</a>

       <a href="/edit?id=<%= item.id %>">Update</a>

     </td>
  </tr>
  <% });  %>
</table>
